<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root spacing="4.0" type="HBox" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" spacing="4.0">
         <children>
            <Label text="Min Value" />
            <Spinner fx:id="min" minWidth="50.0" prefHeight="26.0" prefWidth="100.0" >
            </Spinner>
         </children>
      </VBox>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="10.0" minWidth="50.0" spacing="4.0">
         <children>
            <Label text="Max Value" />
            <Spinner fx:id="max" prefWidth="100.0" >
            </Spinner>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
   </padding>
</fx:root>
